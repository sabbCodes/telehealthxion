{
  "contract_name": "payments",
  "contract_version": "0.1.0",
  "idl_version": "1.0.0",
  "instantiate": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "InstantiateMsg",
    "type": "object",
    "required": [
      "platform_fee_percent",
      "platform_wallet"
    ],
    "properties": {
      "platform_fee_percent": {
        "type": "integer",
        "format": "uint64",
        "minimum": 0.0
      },
      "platform_wallet": {
        "type": "string"
      }
    }
  },
  "execute": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ExecuteMsg",
    "oneOf": [
      {
        "type": "object",
        "required": [
          "distribute_payment"
        ],
        "properties": {
          "distribute_payment": {
            "type": "object",
            "required": [
              "amount",
              "doctor"
            ],
            "properties": {
              "amount": {
                "type": "integer",
                "format": "uint128",
                "minimum": 0.0
              },
              "doctor": {
                "type": "string"
              }
            }
          }
        },
        "additionalProperties": false
      }
    ]
  },
  "query": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "QueryMsg",
    "oneOf": [
      {
        "type": "object",
        "required": [
          "get_config"
        ],
        "properties": {
          "get_config": {
            "type": "object"
          }
        },
        "additionalProperties": false
      }
    ]
  },
  "migrate": null,
  "sudo": null,
  "responses": {
    "get_config": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "ConfigResponse",
      "type": "object",
      "required": [
        "platform_fee_percent",
        "platform_wallet"
      ],
      "properties": {
        "platform_fee_percent": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "platform_wallet": {
          "type": "string"
        }
      }
    }
  }
}
